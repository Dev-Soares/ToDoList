generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id          Int      @id @default(autoincrement())
  title       String?
  content     String?
  isCompleted Boolean  @default(false)
  createdAt   DateTime @default(now())
}
